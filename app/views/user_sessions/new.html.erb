<h1>New user_session</h1>

  <% form_for @user_session do |f| %>
    <%= f.error_messages %>
    <%= f.label :email %><br />
    <%= f.text_field :login %><br />
    <br />
    <%= f.label :password %><br />
    <%= f.password_field :password %><br />
    <br />
    <%= f.submit "Login" %>
  <% end %>
